{
  "name": "my-node-fp",
  "version": "0.9.0",
  "description": "Simple node programming utility",
  "scripts": {
    "script": "cross-env NODE_ENV=production TS_NODE_PROJECT=tsconfig.json ts-node -r tsconfig-paths/register",
    "lint": "eslint --quiet --ext ts,tsx ./src/*",
    "clean": "rimraf dist artifact",
    "ctix": "ctix single -p tsconfig.json -o src -w",
    "ctix:remove": "ctix remove -p tsconfig.json -b",
    "prepub:prod": "npm run rollup:prod",
    "pub:prod": "npm publish --access=public",
    "postpub:prod": "npm run ctix:remove",
    "build": "tsc --incremental --noemit -p tsconfig.json",
    "prerollup:prod": "npm run clean && npm run ctix",
    "rollup:prod": "cross-env NODE_ENV=develop rollup --config ./.configs/rollup.config.prod.ts  --configPlugin ts",
    "prerollup:dev": "npm run clean && npm run ctix",
    "rollup:dev": "cross-env NODE_ENV=develop rollup --config ./.configs/rollup.config.dev.ts  --configPlugin ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/imjuni/my-node-fp.git"
  },
  "author": "ByungJoon Lee <imjuni+github@gmail.com>",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "typings": "./dist/esm/index.d.ts",
  "module": "./dist/esm/index.js",
  "files": [
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/imjuni/my-node-fp/issues"
  },
  "homepage": "https://github.com/imjuni/my-node-fp#readme",
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/node": "^18.7.14",
    "@typescript-eslint/eslint-plugin": "^5.36.1",
    "@typescript-eslint/parser": "^5.36.1",
    "cross-env": "^7.0.3",
    "ctix": "^1.3.4",
    "eslint": "^8.23.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsdoc": "^39.3.6",
    "eslint-plugin-prefer-arrow": "^1.2.3",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.6.1",
    "prettier-eslint": "^15.0.1",
    "read-pkg": "^5.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.0",
    "rollup-plugin-sourcemaps": "^0.6.3",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-ts": "^3.0.2",
    "ts-jest": "^28.0.8",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.2"
  },
  "dependencies": {
    "my-easy-fp": "^0.14.0"
  }
}
